<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.mobile6.ui.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_testDialogFragment"
            app:destination="@id/testDialogFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/statisticsFragment"
        android:name="com.example.mobile6.ui.statistics.StatisticsFragment"
        android:label="fragment_statistics"
        tools:layout="@layout/fragment_statistics" />
    <fragment
        android:id="@+id/appointmentsFragment"
        android:name="com.example.mobile6.ui.appointments.AppointmentsFragment"
        android:label="fragment_appointments"
        tools:layout="@layout/fragment_appointments" />
    <fragment
        android:id="@+id/prescriptionsFragment"
        android:name="com.example.mobile6.ui.prescriptions.PrescriptionsFragment"
        android:label="fragment_prescriptions"
        tools:layout="@layout/fragment_prescriptions" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.mobile6.ui.profile.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/splashFragment"
        android:name="com.example.mobile6.ui.splash.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash">
        <action
            android:id="@+id/action_splashFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/splashFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/signInFragment"
        android:name="com.example.mobile6.ui.sign_in.SignInFragment"
        android:label="fragment_sign_in"
        tools:layout="@layout/fragment_sign_in">
        <action
            android:id="@+id/action_signInFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/signInFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/medicineSearchFragment"
        android:name="com.example.mobile6.ui.medicine.MedicineSearchFragment"
        android:label="MedicineSearchFragment"
        tools:layout="@layout/fragment_medicine_search">
        <action
            android:id="@+id/action_medicineSearchFragment_to_medicineDetailFragment"
            app:destination="@id/medicineDetailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/medicineDetailFragment"
        android:name="com.example.mobile6.ui.medicine.MedicineDetailFragment"
        android:label="fragment_medicine_detail"
        tools:layout="@layout/fragment_medicine_detail">
        <action
            android:id="@+id/action_medicineDetailFragment_to_medicationWarningDialogFragment"
            app:destination="@id/medicationWarningDialogFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <dialog
        android:id="@+id/medicationWarningDialogFragment"
        android:name="com.example.mobile6.ui.medicine.MedicationWarningDialogFragment"
        android:label="MedicationWarningDialogFragment"
        tools:layout="@layout/fragment_medication_warning_dialog" />
    <fragment
        android:id="@+id/doctorSpecialtyFragment"
        android:name="com.example.mobile6.ui.doctor.doctor_specialty.DoctorSpecialtyFragment"
        android:label="fragment_doctor_specialty"
        tools:layout="@layout/fragment_doctor_specialty">
        <action
            android:id="@+id/action_doctorSpecialtyFragment_to_doctorListFragment"
            app:destination="@id/doctorListFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/doctorListFragment"
        android:name="com.example.mobile6.ui.doctor.doctor_list.DoctorListFragment"
        android:label="fragment_doctor_list"
        tools:layout="@layout/fragment_doctor_list">
        <action
            android:id="@+id/action_doctorListFragment_to_doctorDetailFragment"
            app:destination="@id/doctorDetailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/doctorDetailFragment"
        android:name="com.example.mobile6.ui.doctor.doctor_detail.DoctorDetailFragment"
        android:label="fragment_doctor_detail"
        tools:layout="@layout/fragment_doctor_detail" />
    <dialog
        android:id="@+id/testDialogFragment"
        android:name="com.example.mobile6.ui.test.TestDialogFragment"
        android:label="fragment_test_dialog"
        tools:layout="@layout/fragment_test_dialog" />
    <fragment
        android:id="@+id/profileCompletionFragment"
        android:name="com.example.mobile6.ui.profile.ProfileCompletionFragment"
        android:label="fragment_profile_completion"
        tools:layout="@layout/fragment_profile_completion" >
        <action
            android:id="@+id/action_profileCompletionFragment_to_genderSelectDialogFragment"
            app:destination="@id/genderSelectDialogFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.example.mobile6.ui.auth.register.RegisterFragment"
        android:label="RegisterFragment" >
        <action
            android:id="@+id/action_registerFragment_to_profileCompletionFragment"
            app:destination="@id/profileCompletionFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_registerFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.mobile6.ui.auth.login.LoginFragment"
        android:label="LoginFragment" />
    <dialog
        android:id="@+id/genderSelectDialogFragment"
        android:name="com.example.mobile6.ui.profile.GenderSelectDialogFragment"
        android:label="GenderSelectDialogFragment" />
</navigation>